<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/page9"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:fitsSystemWindows="true"
    android:orientation="vertical">
    <!--标题栏-->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_50dp"
        android:background="@color/deepSkyBlue"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/ks_page9_back"
            android:layout_width="@dimen/size_50dp"
            android:layout_height="match_parent">

            <ImageView style="@style/Icon_back" />
        </LinearLayout>

        <TextView
            android:id="@+id/ks_page9_tv1"
            style="@style/Middle_content" />
    </RelativeLayout>
    <!--内容区域-->
    <com.example.wdks.newwdksapp.tools.MySwipeRefreshLayout
        android:id="@+id/ks_page9_free_video_swRefresh"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ListView
            android:id="@+id/ks_page9_free_video_listView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/white">

        </ListView>
    </com.example.wdks.newwdksapp.tools.MySwipeRefreshLayout>

</LinearLayout>
